{
  "title": "RTC GPU Media - SLOs",
  "panels": [
    {
      "type": "graph",
      "title": "p95 request latency (s)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(app_request_latency_seconds_bucket[5m])) by (le))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Requests per second",
      "targets": [
        {
          "expr": "sum(rate(app_requests_total[1m]))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "GPU Utilization (%)",
      "targets": [
        {
          "expr": "avg(DCGM_FI_DEV_GPU_UTIL)"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Pod replicas",
      "targets": [
        {
          "expr": "kube_deployment_status_replicas{deployment=\"gpu-media\",namespace=\"rtc\"}"
        }
      ]
    }
  ],
  "schemaVersion": 27
}
